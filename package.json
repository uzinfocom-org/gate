{
  "name": "gatesite",
  "version": "1.0.0",
  "description": "Gate website for Uzinfocom Open Source resources",
  "main": "src/js/index.js",
  "scripts": {
    "start": "parcel src/index.html --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/index.html --no-content-hash --no-minify --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/uzinfocom-org/gate.git"
  },
  "keywords": [],
  "author": "Uzinfocom Open Source",
  "license": "MIT",
  "homepage": "http://gate.oss.uzinfocom.uz",
  "bugs": {
    "url": "https://github.com/uzinfocom-org/gate/issues"
  },
  "dependencies": {
    "gsap": "^3.6.0",
    "parcel-bundler": "^1.12.4",
    "splitting": "^1.0.6"
  }
}
